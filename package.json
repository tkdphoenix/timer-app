{
  "name": "timer-app",
  "charset": "utf-8",
  "version": "1.0.0",
  "description": "an app that times how long tasks take",
  "main": "timer-app",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path ./tools/.eslintignore",
    "format:prettier": "prettier \"**/*.js\" --write --ignore-path ./tools/.prettierignore",
    "format": "npm run format:eslint && npm run format:prettier",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path ./tools/.eslintignore",
    "lint:prettier": "prettier \"**/*.js\" --check --ignore-path ./tools/.prettierignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "start": "webpack-dev-server --history-api-fallback --hot --inline --config ./tools/webpack.dev.js",
    "test": "test",
    "update": "npx npm-check -u"
  },
  "keywords": [
    "timer"
  ],
  "author": "Joel Grissom",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkdphoenix/timer-app.git"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "conventional-changelog-conventionalcommits": "^4.3.0",
    "lit-element": "^2.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-async-generator-functions": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@babel/runtime-corejs3": "^7.10.5",
    "@commitlint/cli": "^9.1.2",
    "@open-wc/eslint-config": "^2.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitlint": "^9.0.1",
    "cypress": "^4.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-lit": "^1.2.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "webpack-dev-server": "^3.11.0"
  },
  "directories": {
    "images": "images",
    "output": "dist",
    "redux": "redux",
    "source": "src",
    "test": "test",
    "tools": "tools"
  },
  "eslintConfig": {
    "extends": [
      "./tools/.eslintrc.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "./tools/commitlint.config"
    ]
  },
  "prettier": "./tools/prettier.config"
}
